@charset "utf-8";

body,div,h1,h2,h3,ul,li,dt,dd,dl,input,img,a,p,span,strong,
label,table,caption,legend,th,td,thead,tbody,tfoot,address,
form,fieldset,select,header,nav,section,article,aside,small,footer{margin:0; padding:0}
header,footer,section,nav,article{display:block;}

body,input,select{font-size:12px; font-family:"나눔고딕",NanumGothic,"돋움",dotum,sans-serif; color:#333;}

ul{list-style:none;}
img,fieldset{border:none;}
img,input,select{vertical-align:middle}
a:link, a:visited{text-decoration:none; color:#333}
a:hover, a:active{text-decoration:underline; color:#333}
address{font-style:normal;}

.hide{font-size:0; width:1px; height:1px; line-height:0; overflow:hidden; position:absolute; 
visibility:hidden; left:-999px;}

#skipNavi{position:absolute; z-index:999;}
#skipNavi a{position:absolute; top:-100px;}
#skipNavi a:hover,
#skipNavi a:active,
#skipNavi a:focus{position:absolute; width:300px; height:20px; color:#fff; 
					background:#333; font-size:18px; font-weight:bold; top:0;
						display:block; text-align:center; padding:10px;}






